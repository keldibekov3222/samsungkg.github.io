function isSvgElement(o){return Boolean(window.SVGElement&&o[0]instanceof SVGElement)}function isMouseEvent(o){return Boolean(o&&"number"==typeof o.pageX)}function initTracking(){session.mouseTrackingActive||(session.mouseTrackingActive=!0,getViewportDimensions(),$(getViewportDimensions),$document.on("mousemove"+EVENT_NAMESPACE,trackMouse),$window.on("resize"+EVENT_NAMESPACE,trackResize),$window.on("scroll"+EVENT_NAMESPACE,trackScroll))}function getViewportDimensions(){session.scrollLeft=$window.scrollLeft(),session.scrollTop=$window.scrollTop(),session.windowWidth=$window.width(),session.windowHeight=$window.height()}function trackResize(){session.windowWidth=$window.width(),session.windowHeight=$window.height()}function trackScroll(){var o=$window.scrollLeft(),s=$window.scrollTop();o!==session.scrollLeft&&(session.currentX+=o-session.scrollLeft,session.scrollLeft=o),s!==session.scrollTop&&(session.currentY+=s-session.scrollTop,session.scrollTop=s)}function trackMouse(o){session.currentX=o.pageX,session.currentY=o.pageY}function isMouseOver(o){var s=o.offset(),n=o[0].getBoundingClientRect(),i=n.right-n.left,e=n.bottom-n.top;return session.currentX>=s.left&&session.currentX<=s.left+i&&session.currentY>=s.top&&session.currentY<=s.top+e}function getTooltipContent(o){var s,n,i=o.data(DATA_POWERTIP),e=o.data(DATA_POWERTIPJQ),t=o.data(DATA_POWERTIPTARGET);return i?($.isFunction(i)&&(i=i.call(o[0])),n=i):e?($.isFunction(e)&&(e=e.call(o[0])),e.length>0&&(n=e.clone(!0,!0))):t&&(s=$("#"+t),s.length>0&&(n=s.html())),n}function getViewportCollisions(o,s,n){var i=session.scrollTop,e=session.scrollLeft,t=i+session.windowHeight,r=e+session.windowWidth,l=Collision.none;return(o.top<i||Math.abs(o.bottom-session.windowHeight)-n<i)&&(l|=Collision.top),(o.top+n>t||Math.abs(o.bottom-session.windowHeight)>t)&&(l|=Collision.bottom),(o.left<e||o.right+s>r)&&(l|=Collision.left),(o.left+s>r||o.right<e)&&(l|=Collision.right),l}function countFlags(o){for(var s=0;o;)o&=o-1,s++;return s}